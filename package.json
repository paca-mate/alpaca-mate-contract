{
  "name": "alapaca-mate-contract",
  "version": "0.0.1",
  "scripts": {
    "compile5": "hardhat typechain --config hardhat.config.5.ts",
    "compile6": "hardhat typechain --config hardhat.config.ts",
    "compile": "yarn run compile5 && yarn run compile6",
    "mainnet": "hardhat console --no-compile --network mainnet",
    "test": "hardhat test",
    "testnet": "hardhat console --no-compile --network testnet",
    "coverage": "hardhat coverage",
    "solhint": "./node_modules/.bin/solhint -f table contracts/**/*.sol",
    "prettier:solidity": "./node_modules/.bin/prettier --write contracts/**/*.sol",
    "deploy:testnet:mx-alpaca": "hardhat --network testnet deploy --no-compile --reset --tags mxALPACA",
    "deploy:testnet:grass-maxi": "hardhat --network testnet deploy --no-compile --reset --tags GrassMaxi",
    "deploy:testnet:mx-alpaca-set-operator": "hardhat --network testnet deploy --no-compile --reset --tags mxALPACASetOperator",
    "verify:testnet:mx-alpaca": "hardhat run --network testnet --no-compile verify/verify-mx-alpaca.ts",
    "verify:testnet:grass-maxi": "hardhat run --network testnet --no-compile verify/verify-grass-maxi.ts"
  },
  "devDependencies": {
    "@alpaca-finance/alpaca-contract": "1.2.23",
    "@alpaca-finance/xalpaca-contract": "^0.0.5",
    "@ethersproject/bignumber": "^5.0.14",
    "@nomiclabs/hardhat-ethers": "^2.0.1",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "4.3.2",
    "@openzeppelin/contracts-upgradeable": "4.3.2",
    "@openzeppelin/hardhat-upgrades": "^1.9.0",
    "@pancakeswap-libs/pancake-swap-core": "^0.1.0",
    "@typechain/ethers-v5": "^9.0.0",
    "@typechain/hardhat": "^4.0.0",
    "@types/chai": "^4.2.15",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.28",
    "chai": "^4.3.0",
    "chai-bn": "^0.2.1",
    "dotenv": "^8.2.0",
    "ethereum-waffle": "^3.4.0",
    "ethereumjs-util": "^7.0.10",
    "ethers": "^5.0.31",
    "hardhat": "^2.0.10",
    "hardhat-deploy": "^0.7.0-beta.45",
    "mocha": "^9.2.0",
    "prettier": "^2.3.0",
    "prettier-plugin-solidity": "^1.0.0-beta.10",
    "solhint": "^3.3.4",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.15",
    "synthetix": "^2.39.2",
    "ts-generator": "^0.1.1",
    "ts-node": "^9.1.1",
    "typechain": "^7.0.0",
    "typescript": "^4.1.5"
  },
  "files": [
    "typechain",
    "artifacts",
    "contracts",
    ".testnet.json",
    ".staging.json",
    ".mainnet.json"
  ],
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@openzeppelin/test-helpers": "^0.5.15"
  }
}
